    "opn-cli": "^3.1.0"
  },
  "scripts": {
    "test": "mocha src/**/*.test.js",
    "dev": "nodemon .",
    "test:watch" : "mocha -w src/**/*.test.js",
    "test:awesome" : "mocha src/**/*.test.js -w -R mochawesome --reporter-options reportDir=report,
    reportName=index && live-server report",
    "test:nyan": "mocha -w src/**/*.test.js -R nyan",
    "test:custom" : "mocha -w src/**/*.test.js -R landing"
  },
